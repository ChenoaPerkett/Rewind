{
  "name": "rewind",
  "version": "1.0.0",
  "description": "Playlist Sharing Website",
  "git repository": "https://github.com/ChenoaPerkett/Rewind",
  "main": "frontend/src/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.mjs && babel backend/server.js -d backend/dist",
    "start": "npm run css && npm run build && node backend/dist/server.js",
    "css": "npx tailwindcss -i ./frontend/src/index.css -o ./frontend/public/output.css",
    "dev": "concurrently \"npm run css\" \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "webpack serve --config webpack.config.mjs --mode development",
    "dev:backend": "nodemon backend/server.js --watch backend"
  },
  "author": "Chenoa Perkett",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.0.1",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "nodemon": "^3.0.0",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.13",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "copy-webpack-plugin": "^12.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.7.0",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2"
  }
}
